mixin page_title(title, subtitle, ...items)
  div.container.clearfix
    h1=title
    span=subtitle
    ol.breadcrumb.breadcrumb-sf
      each item in items
        if item[2]
          li.active=item[0]
        else
          li
            a(href=item[1])=item[0]
